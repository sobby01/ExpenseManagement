<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ExpenseManagement.ViewModel"
                    xmlns:vw="clr-namespace:ExpenseManagement.View">

        <!-- 
  This template applies an AllCustomersView to an instance 
  of the AllCustomersViewModel class shown in the main window.
  -->
    <DataTemplate DataType="{x:Type vm:RegisterUViewModel}">
        <vw:RegisterU />
    </DataTemplate>

        <!-- 
  This template applies a CustomerView to an instance  
  of the CustomerViewModel class shown in the main window.
  -->
    <DataTemplate DataType="{x:Type vm:XpenseManagementViewModel}">
        <vw:XpenseManagement />
    </DataTemplate>

    </ResourceDictionary>